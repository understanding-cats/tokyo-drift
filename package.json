{
  "name": "tokyo-drift",
  "productName": "tokyo-drift",
  "version": "1.0.0",
  "description": "Pomodoro 2.0",
  "main": ".src/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .js .",
    "doc": "documentation build src/** -f html -o docs",
    "test:all": "xvfb-maybe mocha --recursive --exit",
    "test:unit": "xvfb-maybe mocha --recursive test/unit",
    "style": "prettier --write .",
    "stylecheck": "prettier --check ."
  },
  "keywords": [],
  "author": {
    "name": "emugnier",
    "email": "emugnier@ucsd.edu"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "tokyo_drift"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": []
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "css-loader": "^4.3.0",
    "documentation": "^13.1.0",
    "electron": "^10.1.5",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.2.1",
    "node-loader": "^1.0.2",
    "prettier": "^2.2.1",
    "spectron": "^12.0.0",
    "style-loader": "^1.3.0",
    "xvfb-maybe": "^0.2.1"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^3.5.1"
  }
}
